{
  "name": "challenge-10-ch-enzovignolo",
  "version": "1.0.0",
  "description": "Desafio entregable para CH BackEnd Course",
  "main": "server.js",
  "scripts": {
    "test": "jest --detectOpenHandles",
    "test:auto": "node src/benchmark.js",
    "start": "node src/server.js",
    "dev": "nodemon src/server.js",
    "0xstart": "0x src/server.js",
    "forever": "forever start -w src/server.js",
    "forever:stop": "forever stopall",
    "pm2:fork": "pm2 start src/server.js --watch && pm2 list",
    "pm2:cluster": "pm2 start src/server.js --watch -i max && pm2 list",
    "pm2:stop": "pm2 stop all",
    "pm2:delete": "pm2 delete all",
    "pm2:nginx": "pm2 start src/server.js -n Server_1 -i max -w -- PORT=8081 && pm2 start src/server.js -n Server_2 -w -- PORT=8082"
  },
  "author": "Enzo Vignolo",
  "license": "ISC",
  "dependencies": {
    "@shelf/jest-mongodb": "^2.1.0",
    "autocannon": "^7.5.0",
    "compression": "^1.7.4",
    "connect-mongo": "^4.6.0",
    "core-js": "^3.16.0",
    "cors": "^2.8.5",
    "dotenv": "^10.0.0",
    "ejs": "^3.1.6",
    "express": "^4.17.1",
    "express-handlebars": "^5.3.2",
    "express-session": "^1.17.2",
    "knex": "^0.95.11",
    "log4js": "^6.3.0",
    "mongoose": "^6.0.3",
    "mysql": "^2.18.1",
    "mysql2": "^2.3.0",
    "nodemailer": "^6.7.0",
    "normalizr": "^3.6.1",
    "passport": "^0.4.1",
    "passport-facebook": "^3.0.0",
    "passport-local": "^1.0.0",
    "pug": "^3.0.2",
    "regenerator-runtime": "^0.13.9",
    "socket.io": "^4.1.3",
    "twilio": "^3.70.0",
    "typescript": "^4.3.5",
    "winston": "^3.3.3"
  },
  "devDependencies": {
    "@babel/cli": "^7.14.8",
    "@babel/core": "^7.14.8",
    "@babel/preset-env": "^7.14.9",
    "tsc": "^2.0.3"
  }
}
