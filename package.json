	"description": "Desafio entregable para CH BackEnd Course",
	"main": "server.js",
	"scripts": {
		"test": "echo \"Error: no test specified\" && exit 1",
		"test:auto": "node benchmark.js",
		"start": "nodemon server.js",
		"0xstart": "0x server.js",
		"forever": "forever start -w server.js",
		"forever:stop": "forever stopall",
		"pm2:fork": "pm2 start server.js --watch && pm2 list",
		"pm2:cluster": "pm2 start server.js --watch -i max && pm2 list",
		"pm2:stop": "pm2 stop all",
		"pm2:delete": "pm2 delete all",
		"pm2:nginx": "pm2 start server.js -n Server_1 -i max -w -- PORT=8081 && pm2 start server.js -n Server_2 -w -- PORT=8082"
	},
